{
  "rules": {
    "linebreak-style": 0,
    "max-lines-per-function": ["error", { "max" : 10 }]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"]
}
